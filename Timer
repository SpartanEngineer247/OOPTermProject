
#include <chrono>
#include <iostream>
#include <future>
#include <time.h>
#include <thread>
using namespace std;

void timer(int s) {
  //  cout<<"enter number of seconds"<<endl;
      // int seconds;
    //   cin>>seconds;
    std::cout << "Start\n";
    for(int i=0;i<s;++i)
    {
        std::cout << (i+1) << '\n';
        std::this_thread::sleep_for(std::chrono::seconds(1));
    }
    std::cout << "DONE\n";
}
